{
  "manifest_version": 2,
  "name": "First Chrome Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://medium.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "animal-paw-print.png"
  }
}