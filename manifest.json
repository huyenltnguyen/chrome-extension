{
  "manifest_version": 2,
  "name": "First Chrome Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "animal-paw-print.png",
    "default_popup": "popup.html",
    "default_title": "I'm a popup"
  }
}